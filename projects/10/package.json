{
  "name": "10",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./",
    "tokenized:Square": "ts-node ./ ./Square --skipCompileXML && sh ../../tools/TextComparer.sh ./TokenizeResults/MainT.xml ./Square/MainT.xml",
    "tokenized:ArrayTest": "ts-node ./ ./ArrayTest --skipCompileXML && sh ../../tools/TextComparer.sh ./TokenizeResults/MainT.xml ./ArrayTest/MainT.xml",
    "tokenized:all": "yarn tokenized:Square && yarn tokenized:ArrayTest",
    "compile:ExpressionLessSquare": "ts-node ./ ./ExpressionLessSquare --skipTokenizedXML && sh ../../tools/TextComparer.sh ./CompileResults/Main.xml ./ExpressionLessSquare/Main.xml && sh ../../tools/TextComparer.sh ./CompileResults/Square.xml ./ExpressionLessSquare/Square.xml && sh ../../tools/TextComparer.sh ./CompileResults/SquareGame.xml ./ExpressionLessSquare/SquareGame.xml",
    "compile:Square": "ts-node ./ ./Square --skipTokenizedXML && sh ../../tools/TextComparer.sh ./CompileResults/Main.xml ./Square/Main.xml && sh ../../tools/TextComparer.sh ./CompileResults/Square.xml ./Square/Square.xml && sh ../../tools/TextComparer.sh ./CompileResults/SquareGame.xml ./Square/SquareGame.xml",
    "compile:ArrayTest": "ts-node ./ ./ArrayTest --skipTokenizedXML && sh ../../tools/TextComparer.sh ./ArrayTest/Main.xml ./ArrayTest/Main.xml",
    "compile:all": "yarn compile:ExpressionLessSquare && yarn compile:Square && yarn compile:ArrayTest",
    "test": "yarn tokenized:all && yarn compile:all"
  },
  "devDependencies": {
    "@types/node": "^13.13.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
