{
  "name": "10",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./",
    "tokenized:Square": "ts-node ./ ./ProgramSamples/Square --skipCompileXML && sh ../../tools/TextComparer.sh ./ProgramResults/Tokenizer/MainT.xml ./ProgramSamples/Square/MainT.xml",
    "tokenized:ArrayTest": "ts-node ./ ./ProgramSamples/ArrayTest --skipCompileXML && sh ../../tools/TextComparer.sh ./ProgramResults/Tokenizer/MainT.xml ./ProgramSamples/ArrayTest/MainT.xml",
    "tokenized:all": "yarn tokenized:Square && yarn tokenized:ArrayTest",
    "compile:ExpressionLessSquare": "ts-node ./ ./ProgramSamples/ExpressionLessSquare --skipTokenizedXML && sh ../../tools/TextComparer.sh ./ProgramResults/Compiler/Main.xml ./ProgramSamples/ExpressionLessSquare/Main.xml && sh ../../tools/TextComparer.sh ./ProgramResults/Compiler/Square.xml ./ProgramSamples/ExpressionLessSquare/Square.xml && sh ../../tools/TextComparer.sh ./ProgramResults/Compiler/SquareGame.xml ./ProgramSamples/ExpressionLessSquare/SquareGame.xml",
    "compile:Square": "ts-node ./ ./ProgramSamples/Square --skipTokenizedXML && sh ../../tools/TextComparer.sh ./ProgramResults/Compiler/Main.xml ./ProgramSamples/Square/Main.xml && sh ../../tools/TextComparer.sh ./ProgramResults/Compiler/Square.xml ./ProgramSamples/Square/Square.xml && sh ../../tools/TextComparer.sh ./ProgramResults/Compiler/SquareGame.xml ./ProgramSamples/Square/SquareGame.xml",
    "compile:ArrayTest": "ts-node ./ ./ProgramSamples/ArrayTest --skipTokenizedXML && sh ../../tools/TextComparer.sh ./ProgramResults/Compiler/Main.xml ./ProgramSamples/ArrayTest/Main.xml",
    "compile:all": "yarn compile:ExpressionLessSquare && yarn compile:Square && yarn compile:ArrayTest",
    "test": "yarn tokenized:all && yarn compile:all"
  },
  "devDependencies": {
    "@types/node": "^13.13.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
